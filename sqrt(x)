class Solution {
public:
    int sqrt(int x) {
        if(x==1) return 1;
        int tail=x, head=1, mid,tmp;
        while(head<tail){
            mid=head+((tail-head)>>1);
            //tmp=mid*mid; è¶Šç•Œ
            tmp=x/mid;
            if(mid>tmp) tail=mid;
            else if(mid<tmp) head=mid+1;
            else return mid;
        }
        return --head;
    }
};
